alias e="open -a /Applications/Emacs.app"
alias rmr="rm -rf"

path=(. $HOME/bin /usr/local/sbin /usr/local/bin /sbin /bin /usr/sbin /usr/bin /usr/games /usr/X11R6/bin $HOME/bin/depot_tools)

# 補完の時に大文字小文字を区別しない
zstyle ':completion:*' matcher-list 'm:{a-z}={A-Z}'

#プロンプト
PROMPT="%{[35m%}%U$USER@%m%u%%%{[m%} "
RPROMPT='%{[34m%}[%~]%{[m%}'

